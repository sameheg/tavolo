{
  "name": "cafesaas/app",
  "type": "project",
  "require": {
    "php": "^8.3",
    "laravel/framework": "^12.0",
    "laravel/sanctum": "^4.0",
    "laravel/pennant": "^1.0",
    "stancl/tenancy": "^4.0",
    "open-telemetry/opentelemetry-laravel": "^1.0",
    "cafesaas/kernel": "*",
    "cafesaas/contracts": "*",
    "cafesaas/toolkit": "*",
    "cafesaas/observability": "*",
    "cafesaas/pos": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "packages/*",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "modules/*",
      "options": {
        "symlink": true
      }
    }
  ],
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php artisan package:discover --ansi || true"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}